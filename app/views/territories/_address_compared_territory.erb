 <% if @territory.is_dynamic? %>


     <%if @territory.coordinates.count < 1%>

         <img src="http://maps.google.com/maps/api/staticmap?&size=620x512&markers=color:red|color:red|label:X|


         <%=@address.house_no%>+<%=@address.street%>,<%=@address.city%>,<%=@address.state%> |&markers=color:red|color:red|label:T| <%=@territory.center_coordinate%>


         &zoom=16&maptype=<%=@territory.maptype%>&sensor=false&key=ABQIAAAARGsYqr1Qe8aHLxBzDPVg8RSRXWCxfBZQxUPFeO1fEKXGT2aXExSjBmp62ioToSIU5gkCv_p99qjqjg"> </img>
         <%end%>


       <%if @territory.coordinates.count > 1%>

       <img src="http://maps.google.com/maps/api/staticmap?&markers=color:red|color:red|label:X|

          <% if @address.alt_house_no.nil? %>
          <%=@address.house_no%>+<%=@address.street%>,<%=@address.city%>,<%=@address.state%>
          <%else%>
          <%=@address.alt_house_no%>+<%=@address.alt_street%>,<%=@address.alt_city%>,<%=@address.state%>
          <%end%>


       |&path=color:<%=@territory.border_color%>|weight:5|fillcolor:<%=@territory.color%>|



    <%for cord in @territory.coordinates %>

    <%=cord.coordinate%>
    <%= "|" unless cord == @territory.coordinates.last %>
    <%end%>
    &markers=color:red|color:red|label:T| <%=@territory.center_coordinate%>&size=620x512&maptype=<%=@territory.maptype%>&sensor=false&key=ABQIAAAARGsYqr1Qe8aHLxBzDPVg8RSRXWCxfBZQxUPFeO1fEKXGT2aXExSjBmp62ioToSIU5gkCv_p99qjqjg">
    </img>


    <%end%>

<%else%>

   <%if @territory.coordinates.count > 1%>

       <img src="http://maps.google.com/maps/api/staticmap?&markers=color:red|color:red|label:X|

          <% if @address.alt_house_no.nil? %>
          <%=@address.house_no%>+<%=@address.street%>,<%=@address.city%>,<%=@address.state%>
          <%else%>
          <%=@address.alt_house_no%>+<%=@address.alt_street%>,<%=@address.alt_city%>,<%=@address.state%>
          <%end%>


       |&path=color:<%=@territory.border_color%>|weight:5|fillcolor:<%=@territory.color%>|



    <%for cord in @territory.coordinates %>

    <%=cord.coordinate%>
    <%= "|" unless cord == @territory.coordinates.last %>
    <%end%>
    &markers=color:red|color:red|label:T| <%=@territory.center_coordinate%>&size=620x512&maptype=<%=@territory.maptype%>&sensor=false&key=ABQIAAAARGsYqr1Qe8aHLxBzDPVg8RSRXWCxfBZQxUPFeO1fEKXGT2aXExSjBmp62ioToSIU5gkCv_p99qjqjg">
    </img>


    <%else%>



  <% @url = "/images/#{session[:congid]}" + @territory.img_url  %>
 <%=  link_to image_tag(@url,:size=>"500x300",:title=>"#{t :zoom}"),:action =>"view_ter_image",:imgurl => @url  %>
  <%end%>
  <%end%>


