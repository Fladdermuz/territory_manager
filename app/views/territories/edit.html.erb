
<div class="card">
  <div class="card-header-top card-header-top">
    <table class="table table-noborder table-nopadding">
      <tr>
        <td class="card-title no-border">
          <h2> <%= t :territory %> </h2>
          <% if flash[:notice] %>
              <p class='notice' id="notice"><%= notice %></p>
          <% end %>
        </td>
        <td class="buttons">
           <%= link_to "", territories_path, class: 'btn btn-primary btn-home btn-top' %>
        </td>
      </tr>
    </table>
  </div>
  <div class="card-block">

    <%= render 'form', territory: @territory, zone: @territory.zone %>


  </div>
</div>
